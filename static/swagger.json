{
    "swagger": "2.0",
    "basePath": "/",
    "paths": {
        "/data/{id}/{name}": {
            "parameters": [
                {
                    "name": "id",
                    "in": "path",
                    "required": true,
                    "type": "integer"
                },
                {
                    "name": "name",
                    "in": "path",
                    "required": true,
                    "type": "string"
                }
            ],
            "get": {
                "responses": {
                    "200": {
                        "description": "Data fetched successfully"
                    }
                },
                "description": "Fetches data from PostgreSQL and returns it",
                "operationId": "get_data_resource",
                "tags": [
                    "default"
                ]
            }
        }
    },
    "info": {
        "title": "User/Post",
        "version": "1.0"
    },
    "produces": [
        "application/json"
    ],
    "consumes": [
        "application/json"
    ],
    "tags": [
        {
            "name": "data",
            "description": "changing default NameSpace"
        }
    ],
    "responses": {
        "ParseError": {
            "description": "When a mask can't be parsed"
        },
        "MaskError": {
            "description": "When any error occurs on mask"
        }
    }
}